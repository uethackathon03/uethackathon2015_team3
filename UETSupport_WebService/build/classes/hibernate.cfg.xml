<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
                                         "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory name="">
		<property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
		<property name="hibernate.show_sql">true</property>
		<property name="hibernate.hbm2ddl.auto">update</property>
		<mapping class="com.unbelievable.uetsupport.respsec.TokenInfo" />
		<mapping class="com.unbelievable.uetsupport.models.Student" />
		<mapping class="com.unbelievable.uetsupport.models.Teacher" />
		<mapping class="com.unbelievable.uetsupport.models.Department" />
		<mapping class="com.unbelievable.uetsupport.models.Information" />
		<mapping class="com.unbelievable.uetsupport.models.Answer" />
		<mapping class="com.unbelievable.uetsupport.models.Thread" />
		<mapping class="com.unbelievable.uetsupport.models.Comment" />
		<mapping class="com.unbelievable.uetsupport.models.LikeThread" />
	</session-factory>
</hibernate-configuration>